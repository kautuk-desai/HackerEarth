using System; 

class MyClass {
    public static void Main(string[] args) {
    	int tc= Convert.ToInt32(Console.ReadLine());
        string revstr="";
        for(int j=1;j<=tc;j++)
        {
            string Str = Console.ReadLine();

            for (int i = Str.Length - 1; i >= 0; i--)
            {
				
                revstr = revstr + Str[i];
            }
            Console.WriteLine(revstr);
            revstr="";
        }   
            Console.ReadLine();
    }
}
